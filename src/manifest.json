{
	"name": "QuicKey â€“ The quick tab switcher",
	"short_name": "QuicKey",
	"version": "0.0.14",
	"manifest_version": 2,
	"description": "Add keyboard shortcuts to switch tabs with a Quicksilver-style search or a most-recently-used menu",
	"author": "John Dunning",
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
	"minimum_chrome_version": "45",
	"permissions": [
		"tabs",
		"bookmarks",
		"history",
		"storage",
		"sessions",
		"chrome://favicon/"
	],
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "img/icon-19.png",
			"38": "img/icon-38.png"
		},
		"default_title": "QuicKey",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html",
		"persistent": true
	},
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"windows": "Alt+Q",
				"mac": "MacCtrl+Q",
				"chromeos": "Alt+Q",
				"linux": "Alt+Q"
			}
		},
		"1-previous-tab": {
			"suggested_key": {
				"windows": "Alt+A",
				"mac": "MacCtrl+A",
				"chromeos": "Alt+A",
				"linux": "Alt+A"
			},
			"description": "Switch to the previous tab"
		},
		"2-next-tab": {
			"suggested_key": {
				"windows": "Alt+Shift+A",
				"mac": "MacCtrl+Shift+A",
				"chromeos": "Alt+Shift+A",
				"linux": "Alt+Shift+A"
			},
			"description": "Switch to the next tab"
		}
	},
	"offline_enabled": true
}