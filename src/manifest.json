{
	"name": "QuicKey â€“ The quick tab switcher",
	"short_name": "QuicKey",
	"version": "0.0.8",
	"manifest_version": 2,
	"description": "Switch tabs or access bookmarks and history with a Quicksilver-style search",
	"author": "John Dunning",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"minimum_chrome_version": "38",
	"permissions": [
		"tabs",
		"bookmarks",
		"history",
		"chrome://favicon/"
	],
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "img/icon-19.png",
			"38": "img/icon-38.png"
		},
		"default_title": "QuicKey",
		"default_popup": "popup.html"
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"windows": "Alt+Q",
				"mac": "MacCtrl+Q",
				"chromeos": "Alt+Q",
				"linux": "Alt+Q"
			}
		}
	},
	"offline_enabled": true
}